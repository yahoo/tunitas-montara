check_bin_pong = check/bin/pong
check_bin_pong_LDADD = $(lib_libtunitas_montara_la)
check_bin_pong_LDFLAGS = $(CHECK_LDFLAGS_SET)
$(check_bin_pong_OBJECTS): AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
$(check_bin_pong_OBJECTS): AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET)
check_bin_pong_MODULES = $(call HT_SOURCEStoMODULES,$(check_bin_pong_SOURCES))
check_bin_pong_SOURCES = \
  obj/src/tests/pong/c.main.cpp \
  obj/src/tests/pong/main.cpp \
  obj/src/tests/pong/namespace.cpp \
  obj/src/tests/pong/Arguments.cpp \
  obj/src/tests/pong/Pong.cpp \
  obj/src/tests/pong/pause.cpp \
  \
  obj/src/tests/pong/packaging/namespace.cpp \
  obj/src/tests/pong/packaging/pause/namespace.cpp \
  obj/src/tests/pong/packaging/pause/interface.cpp \
  obj/src/tests/pong/packaging/pause/Cleanup.cpp \
  $(end)
$(scold_cxxdir)/tests/pong/dependencies.mk: $(check_bin_pong_SOURCES)
$(check_bin_pong_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(HT_V_COLD) $(call HT_XCPP2CPP, $<)
	$(HT_V_DONE) test -e $@
$(scold_cxxdir)/tests/pong/dependencies.mk:
	$(HT_V_BOTH) $(call HT_DEPENDENCIES, $^)
	$(HT_V_DONE) test -e $@
include $(scold_cxxdir)/tests/pong/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/pong/dependencies.mk
