#!/bin/bash
source ${0%/*}/rigging.sh || exit 70
code=$(curlish \
    -H 'Content-Type: application/json' \
    -X DELETE \
    $URLHOSTPORT/consent \
    ${end})
# This will fail because you cannot say 'DELETE /consent'
# Instead you must say 'DELETE /consent/{consentId}
assert 0 == $? and 405 == ${code?}
